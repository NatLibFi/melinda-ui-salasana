{
  "name": "melinda-ui-salasana",
  "version": "1.0.0",
  "description": "Project base for Melinda UI Salasana",
  "author": "The National Library of Finland",
  "license": "MIT",
  "keywords": [
    "ui-salasana",
    "melinda",
    "natlibfi"
  ],
  "homepage": "https://github.com/NatLibFi/melinda-ui-salasana#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NatLibFi/melinda-ui-salasana.git"
  },
  "bugs": {
    "url": "https://github.com/NatLibFi/melinda-ui-salasana/issues"
  },
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "start": "node dist/index.js",
    "prepare": "npm run build",
    "build": "node build.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "npm run lint",
    "test:base": "node --test --test-force-exit --experimental-test-coverage --test-reporter=spec './src/*.test.mjs'",
    "dev": "cross-env DEBUG=@natlibfi/* NODE_ENV=test node --watch --test --experimental-test-coverage --test-reporter=spec './src/*.test.mjs'"
  },
  "dependencies": {
    "@natlibfi/melinda-backend-commons": "next",
    "@natlibfi/melinda-commons": "next",
    "@natlibfi/melinda-rest-api-client": "next",
    "@natlibfi/melinda-ui-commons": "next",
    "@natlibfi/passport-melinda-aleph": "next",
    "@natlibfi/passport-melinda-jwt": "next",
    "cookie-parser": "^1.4.7",
    "debug": "^4.4.3",
    "express": "^4.19.2",
    "express-handlebars": "^8.0.3",
    "passport": "^0.7.0"
  },
  "devDependencies": {
    "@natlibfi/fixugen": "next",
    "@natlibfi/fixura": "next",
    "esbuild": "^0.25.11",
    "eslint": "^9.38.0"
  }
}